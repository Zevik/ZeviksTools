{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "react/no-unescaped-entities": ["error", {"forbid": [">", "}", "'", "\""]}],
    "@next/next/no-img-element": "warn"
  }
}
